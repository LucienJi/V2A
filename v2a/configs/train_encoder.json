{   
    "algo_name": "V2A",
    "experiment": {
        "name": "test",
        "validate": false,
        "logging": {
            "terminal_output_to_txt": true,
            "log_tb": true
        }},
    "train": {
        "data": "/code/V2A/datasets/debug/output_dataset.hdf5",
        "output_dir": "../trained_models_highlevel",
        "num_data_workers": 1,
        "hdf5_cache_mode": "low_dim",
        "hdf5_use_swmr": true,
        "hdf5_normalize_obs": false,
        "hdf5_load_next_obs": false,
        "hdf5_filter_key": "train",
        "hdf5_validation_filter_key": null,
        "seq_length": 5,
        "frame_stack": 1,
        "pad_frame_stack":true,
        "dataset_keys": [
            "actions",
            "rewards",
            "dones"
        ],
        "goal_mode": null,
        "cuda": true,
        "batch_size": 32,
        "num_epochs": 3000,
        "seed": 1
    },
    "algo":{
        "encoder": {
            "n_seq": 5,
            "embedding_dim": 128,
            "heads": 4,
            "n_layer": 2,
            "num_prototypes": 128,
            "tcl":
            {
                "positive_window": 4,
                "negative_window": 12,
                "num_negatives": 16,
                "tcl_coef": 0.1
            },
            "cluster":
            {
                "cluster_coef": 0.1
            },
            "mcr":
            {
                "mcr_coef": 0.1
            },
            "obs_augmentation": {
              "crop": {
                  "type": "RandomResizedCrop", 
                  "size": 224
                },
              "color_jitter": 
                {
                  "brightness": 0.8, 
                  "contrast": 0.8, 
                  "saturation": 0.8, 
                  "hue": 0.2, 
                  "p": 0.8
              },
              "grayscale": 
                {
                  "p": 0.2
                },
              "gaussian_blue": {
                  "kernel_size": 23, 
                  "p": 0.5
                }
          }

        }
    },

    "observation": {
      "modalities": {
        "obs": {
          "low_dim": [
            "robot0_eef_pos",
            "robot0_eef_quat"
          ],
          "rgb": [
            "agentview_image"
          ],
          "depth": [],
          "scan": []
        },
        "goal": {
          "low_dim": [],
          "rgb": [
            "agentview_image"
          ],
          "depth": [],
          "scan": []
        }
      },
      "encoder": {
            "robot_state":[
                "robot0_eef_pos",
                "robot0_eef_quat"
            ],
            "rgb":[
                "agentview_image"
            ]
        }
    }
  }